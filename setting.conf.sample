# mysql conf file path written username and password
MYSQLCONF_FILEPATH='/root/.my.cnf'

# backup target database names
DB_LIST=('sampl_db_name')

# backup period: days
BACKUP_PERIOD=3

# mysqldump options
DUMP_OPTION="--quote-names --opt --skip-lock-tables --single-transaction --flush-logs --master-data=2"
